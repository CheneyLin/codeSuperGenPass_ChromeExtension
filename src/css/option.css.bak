
 html,body{ height: 100%; margin: 0; padding: 0; }
  body{
    width:           900px;
    margin:          0 auto;
    font-family:     sans-serif;
    background:      -webkit-gradient(
                                      linear,
                                      left bottom,
                                      left top,
                                      from(#edeef1),
                                      to(#e3e5ea)
                                    );
    color:           #000;
    text-shadow:     1px 1px 0 rgba(255,255,255,0.75);
    font-weight:     bold;
    -webkit-font-smoothing: subpixel-antialiased;
  }
  #header{
    background:      url('icon_128.png') no-repeat left center;
    padding:         0 0 0 140px;
    height:          100px;
    text-align:      left;
    margin:          0 0 0 50px;
    line-height:     100px;
    font-size:       14px;
  }
  #header a{
    text-decoration: none;
    color:           #000;
  }
  #saved_passwords{
    width:           800px;
    margin:          0 auto;
    padding:         0;
    clear:           both;
    display:         block;
  }
  #saved_passwords ul{
    width:           800px;
    float:           left;
    clear:           both;
    border-bottom:   1px solid rgba(0,0,0,0.3);
    -webkit-box-shadow: 0 1px 0 rgba(255,255,255,0.75);
    margin:          0;
    padding:         10px 0 0 0;
  }
  #saved_passwords ul.deleted{
    background:      rgba(255,128,128,0.25);
  }
  #saved_passwords ul.added{
    background:      rgba(128,255,128,0.25);
  }
  #saved_passwords li{
    float:           left;
    list-style:      none;
    display:         inline;
    margin:          0;
    padding:         0;
    overflow:        hidden;
  }
  #saved_passwords li:nth-child(1){
    font-size:       18px;
    width:           190px;
    padding:         5px;
    margin:          0 20px;
    text-align:      right;
  }
  #saved_passwords li:nth-child(2){
    font-size:       12px;
    font-style:      italic;
    text-shadow:     none;
    width:           20px;
    padding:         8px 0;
    text-align:      center;
  }
  #saved_passwords li:nth-child(3){
    font-size:       12px;
    text-shadow:     none;
    width:           200px;
    text-align:      center;
    padding:         8px 0;
    color:           #aaa;
  }
  #saved_passwords li:nth-child(3) em{
    color:           #000;
    font-style:      normal;
  }
  #saved_passwords li:nth-child(4){
    font-size:       12px;
    text-shadow:     none;
    width:           150px;
    text-align:      center;
    padding:         8px 0;
  }
  #saved_passwords li:nth-child(5){
    font-size:       12px;
    text-shadow:     none;
    width:           180px;
    text-align:      center;
    padding:         8px 0;
  }
  #saved_passwords button{
    width:           125px;
    margin-top:      -8px;
    padding:         5px 0;
    background:      -webkit-gradient(
                   linear,
                   left bottom,
                   left top,
                   from(#c00),
                   to(#f44)
                  );
    -webkit-box-shadow: 1px 1px 0 rgba(255,255,255,0.75);
    border-radius:   40px;
    border:          1px solid #900;
    font-size:       12px;
    font-weight:     bold;
    color:           #200;
    float:           right;
  }
  #saved_passwords button:hover{
    background:      -webkit-gradient(
                   linear,
                   left bottom,
                   left top,
                   from(#e00),
                   to(#f66)
                  );
  }
  #saved_passwords button:focus,#saved_passwords button:active{
    background:      -webkit-gradient(
                   linear,
                   left bottom,
                   left top,
                   from(#f44),
                   to(#c00)
                  );
  }
  .panel{
    border-radius:   30px;
    padding:         0 145px;
    border:          1px solid #000;
    width:           610px;
    margin:          20px 25px;
    clear:           both;
  }
  #general{
    width:           700px;
    padding:         0 100px;
  }
  .panel hr{
    border:          0;
    border-bottom:   1px solid rgba(255,255,255,0.3);
    border-top:      1px solid rgba(0,0,0,0.3);
  }
  .labels{
    margin:          30px 0 5px 0;
    padding:         0;
    display:         block;
    clear:           both;
  }
  .labels label{
    font-style:      italic;
    text-align:      center;
  }
  .labels.one label:nth-child(1){
    padding:         0 30px;
  }
  .labels.one label:nth-child(2){
    padding:         0 55px;
  }
  .labels.one label:nth-child(3){
    padding:         0 88px;
  }
  .labels.two{
    margin-left:     120px;
  }
  .labels.two label:nth-child(1){
    padding:         0 70px;
  }
  .labels.two label:nth-child(2){
    padding:         0 50px;
  }
  #name{
    float:           left;
    margin:          0 5px 0 0;
    padding:         3px;
    font-size:       12px;
    font-weight:     bold;
  }
  #name.bad{
    background:      #fdd;
  }
  .passwordset{
    float:           left;
  }
  .reveal{
    background:      url('data:image/png;base64,%3D')
                                     no-repeat center,
                                    -webkit-gradient(
                                      linear,
                                      left bottom,
                                      left top,
                                      from(#f8b8a2),
                                      to(#fad0c1)
                                    );
    border:          1px solid #19b319;
    border-right:    0;
    width:           23px;
    height:          23px;
    border-radius:   8px 0 0 8px;
    text-align:      center;
    color:           #fff;
    margin:          0;
    padding:         0;
    font-family:     sans-serif;
    font-size:       11px;
    font-weight:     bold;
    line-height:     17px;
    cursor:          pointer;
    float:           left;
  }
  #password,#stealth{
    margin:          0;
    padding:         3px;
    font-size:       12px;
    font-weight:     bold;
    letter-spacing:  3px;
  }
  #buttonset{
    float:           left;
    margin:          0 10px;
    padding:         0;
  }
  #buttonset button{
    float:           left;
    margin:          0;
    padding:         0;
    font-size:       13px;
    font-weight:     bold;
    padding:         3px 8px 4px;
    border:          1px solid #000;
    border-left:     0;
    color:           #fff;
    text-shadow:     -1px -1px 0 #000;
    background:      -webkit-gradient(
                                      linear,
                                      left bottom,
                                      left top,
                                      from(#2b2b2b),
                                      to(#4d4d4d)
                                    );
  }
  #buttonset button:hover{
    background:      -webkit-gradient(
                                      linear,
                                      left bottom,
                                      left top,
                                      from(#444),
                                      to(#666)
                                    );
  }
  #buttonset button:focus, #buttonset button:active{
    background:      -webkit-gradient(
                                      linear,
                                      left bottom,
                                      left top,
                                      from(#4d4d4d),
                                      to(#2b2b2b)
                                    );
  }
  #buttonset button.toggle{
    background:      -webkit-gradient(
                   linear,
                   left bottom,
                   left top,
                   from(#4ae64a),
                   to(#08ce08)
                  );
    text-shadow:     0 0 3px #000;
  }
  #buttonset button:first-child{
    border-left:     1px solid #000;
    border-radius:   13px 0 0 13px;
  }
  #buttonset button:nth-child(4){
    border-radius:   0 13px 13px 0;
  }
  #range_val{
    border:          1px solid #ccc;
    background:      #eee;
    padding:         5px;
    float:           left;
    margin-top:      -3px;
    text-align:      center;
    width:           18px;
    margin-left:     120px;
  }
  #range_val div{
    border-width:    10px;
    border-color:    transparent transparent transparent #ccc;
    border-style:    solid;
    display:         block;
    position:        absolute;
    margin-top:      -20px;
    margin-left:     23px;
  }
  #length{
    float:           left;
    margin:          4px 20px 0 15px;
  }
  #save{
    margin:          30px 200px;
    width:           200px;
    padding:         8px 0;
    background:      -webkit-gradient(
                   linear,
                   left bottom,
                   left top,
                   from(#08ce08),
                   to(#4ae64a)
                  );
    -webkit-box-shadow: 1px 1px 0 rgba(255,255,255,0.75);
    border-radius:   40px;
    border:          1px solid #19b319;
    font-size:       14px;
    font-weight:     bold;
    color:           #023b02;
  }

  #save:hover{
    background:      -webkit-gradient(
                   linear,
                   left bottom,
                   left top,
                   from(#09ef09),
                   to(#68ea68)
                  );
  }

  #save:focus, #save:active{
    background:      -webkit-gradient(
                   linear,
                   left bottom,
                   left top,
                   from(#4ae64a),
                   to(#08ce08)
                  );
  }

  #about{
    width:           450px;
    margin:          0 auto;
    padding:         20px 130px;
    text-align:      center;
    font-size:       10px;
  }
  .answer_box{
    width:           300px;
    height:          98px;
    margin-left:     -155px;
    position:        absolute;
    top:             45px;
    text-align:      center;
    padding:         20px;
    text-shadow:     none;
    font-size:       11px;
    color:           #fff;
    background:      -webkit-gradient(
                                      linear,
                                      left bottom,
                                      left top,
                                      from(#4d4d4d),
                                      to(#2b2b2b)
                                    );
    border-radius:   20px;
  }
  .answer_box div{
    position:        absolute;
    top:             138px;
    left:            148px;
    height:          0;
    width:           0;
    border-width:    7px;
    border-style:    solid;
    border-color:    #4d4d4d transparent transparent transparent;
  }
