.superchromepass_button{
	background:    #b846d3 !important;
	text-align:    center !important;
	color:         #fff !important;
	padding:       0 0 0 2px !important;
	font-family:   sans-serif !important;
	font-size:     11px !important;
	font-weight:   bold !important;
	position:      absolute !important;
	cursor:        pointer !important;
  border-radius: 2px !important;
	z-index:       9999 !important;
}

.superchromepass_button:before {
    content: '';
    position: absolute;
    left: -12px;
    top: 8px;
    width: 0;
    height: 0;
    border: 6px solid transparent;
    border-right-color: #b846d3;
}

.superchromepass_button.expanded{
	height:        auto !important;
	z-index:       999 !important;
}

.superchromepass_button.corner{
	border:        1px solid #b846d3 !important;
}

.superchromepass_button ul{
	margin:        0 !important;
	padding:       0 !important;
	list-style:    none !important;
	color:         #fff !important;
}

.superchromepass_button li{
	margin:        0 !important;
	padding:       0 !important;
	list-style:    none !important;
	color:         #fff !important;
	cursor:        pointer !important;
	font-size:     11px !important;
	line-height:   18px !important;
}

.superchromepass_button li:hover,.superchromepass_button li.active{
	background:    rgba(0,0,0,0.15) !important;
}

.superchromepass_button input{
	padding:       3px !important;
	margin:        3px 6px !important;
	width:         130px !important;
	border:        1px solid #b846d3 !important;
	background:    #fff !important;
	display:       block !important;
	clear:         both !important;
}
.superchromepass_button input.good{
	background:    #effcef !important;
}
.superchromepass_button input.bad{
	background:    #ffe8ea !important;
}

.superchromepass_button p{
	margin:        0 !important;
	padding:       2px 0 !important;
	text-align:    center !important;
	font-weight:   bold !important;
	font-size:     11px !important;
	clear:         both !important;
	display:       block !important;
	line-height:   14px !important;
}
